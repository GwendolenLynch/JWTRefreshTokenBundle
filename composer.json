{
  "name" : "gesdinet/jwt-refresh-token-bundle",
  "description" : "Implements a refresh token system over Json Web Tokens in Symfony",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Marcos GÃ³mez Vilches",
    "email" : "marcos@gesdinet.com"
  }],
  "keywords" : [
    "jwt refresh token bundle symfony json web"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=8.1",
    "doctrine/persistence": "^2.5|^3.0",
    "lexik/jwt-authentication-bundle": "^2.15|^3.0",
    "symfony/config": "^5.4|^6.4|^7.0",
    "symfony/console": "^5.4|^6.4|^7.0",
    "symfony/dependency-injection": "^5.4|^6.4|^7.0",
    "symfony/deprecation-contracts": "^2.1|^3.0",
    "symfony/event-dispatcher": "^5.4|^6.4|^7.0",
    "symfony/http-foundation": "^5.4|^6.4|^7.0",
    "symfony/http-kernel": "^5.4|^6.4|^7.0",
    "symfony/property-access": "^5.4|^6.4|^7.0",
    "symfony/security-bundle": "^5.4|^6.4|^7.0",
    "symfony/security-core": "^5.4|^6.4|^7.0",
    "symfony/security-http": "^5.4|^6.4|^7.0"
  },
  "require-dev": {
    "api-platform/core": "^3.1",
    "doctrine/annotations": "^1.13|^2.0",
    "doctrine/mongodb-odm": "^2.3",
    "doctrine/orm": "^2.12",
    "matthiasnoback/symfony-config-test": "^5.1",
    "matthiasnoback/symfony-dependency-injection-test": "^5.1",
    "phpunit/phpunit": "^9.5",
    "symfony/cache": "^5.4|^6.4|^7.0"
  },
  "conflict": {
    "doctrine/mongodb-odm": "<2.3",
    "doctrine/orm": "<2.12"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    },
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Gesdinet\\JWTRefreshTokenBundle\\Tests\\": "Tests/"
    }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
